{
  "name": "email-send",
  "version": "0.0.1",
  "description": "EMail send",
  "author": "Stanislav V Vyaliy",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "no tests",
    "docker": "docker-compose -p email-send -f devops/docker-compose.yml up -d",
    "docker:down": "docker-compose -p email-send -f devops/docker-compose.yml down --rmi local --volumes"
  },
  "engines": {
    "node": ">= 16",
    "yarn": ">= 1.22"
  },
  "resolutions": {
    "**/**/apollo-graphql": "^0",
    "**/**/apollo-server-core": "^2",
    "**/**/apollo-server-express": "^2",
    "**/**/apollo-server-env": "^3",
    "**/**/apollo-server-types": "^0",
    "**/**/fs-capacitor": "^6",
    "**/**/dotenv": "^9",
    "**/**/tsconfig-paths-webpack-plugin": "^3",
    "**/**/typescript": "^4",
    "**/**/webpack": "^5"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/mapped-types": "^0.4.1",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^7.6.17",
    "apollo-server": "^2.25.0",
    "apollo-server-core": "^2.25.0",
    "apollo-server-express": "^2.25.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.0",
    "ioredis": "^4.27.3",
    "mongoose": "^5.12.12",
    "nest-winston": "^1.5.0",
    "nestjs-redis": "^1.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@types/express": "^4.17.12",
    "@types/graphql-upload": "^8.0.4",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.3",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.3",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-env": "^0.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
